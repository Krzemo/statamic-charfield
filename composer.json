{
  "name": "krzemo/charfield-fieldtype-fork",
  "license": "MIT",
  "type": "statamic-addon",
  "description": "Character counting field for statamic v3",
  "autoload": {
    "psr-4": {
      "AppsWithLove\\Statamic\\CharField\\": "src"
    }
  },
  "authors": [
    {
      "name": "Rémy Böhler"
    }
  ],
  "require": {
    "statamic/cms": "3.4.* || ^4.0"
  },
  "extra": {
    "statamic": {
      "name": "CharField",
      "description": "Character counting field"
    },
    "laravel": {
      "providers": [
        "AppsWithLove\\Statamic\\CharField\\ServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  }
}
